<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Username</th>
      <th>Family name</th>
      <th>Middle names</th>
      <th>First name</th>
      <th>Date of birth</th>
      <th>Country</th>
      <th>State</th>
      <th>Postcode</th>
      <th>Suburb</th>
      <th>Street address</th>
      <th>Level</th>
      <th>Unit</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Ph num</th>
      <th>Mob num</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.user %></td>
        <td><%= profile.username %></td>
        <td><%= profile.family_name %></td>
        <td><%= profile.middle_names %></td>
        <td><%= profile.first_name %></td>
        <td><%= profile.date_of_birth %></td>
        <td><%= profile.country %></td>
        <td><%= profile.state %></td>
        <td><%= profile.postcode %></td>
        <td><%= profile.suburb %></td>
        <td><%= profile.street_address %></td>
        <td><%= profile.level %></td>
        <td><%= profile.unit %></td>
        <td><%= profile.longitude %></td>
        <td><%= profile.latitude %></td>
        <td><%= profile.ph_num %></td>
        <td><%= profile.mob_num %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
